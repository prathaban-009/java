 
import java.util.Arrays;
import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner my=new Scanner (System.in);
        
         
        //array length input
        System.out.print("Enter the length of the array = ");
        int n=my.nextInt();
        System.out.println();

        //array declaration
        int a[]=new int [n];
        
        // Array input using loop
        for(int i=0;i<n;i++)
        {
            a[i]=my.nextInt();
        }
        
         
         
    }
}
   
